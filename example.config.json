{
  "port": 3010,
  "allowed": [ "127.0.0.1", "::ffff:192.168.50.1" ],
  "barcode": {
    "vid": 1529,
    "pid": 8710
  },
  "logs": {
    "info": "logs/info.log",
    "error": "logs/error.log",
    "debug": "logs/debug.log"
  },
  "proxy": {
    "whitelistedBusEvents": [
      "^barcode.data$",
      "^config.translations$",
      "^config.languages$",
      "^config.features$",
      "^config.idle_config$",
      "^frontend.[\\w]+$",
      "^fbs.login.[\\w]+$",
      "^fbs.patron.[\\w]+$",
      "^fbs.checkout.[\\w]+$",
      "^fbs.checkin.[\\w]+$",
      "^fbs.renew.[\\w]+$",
      "^fbs.renew.all.[\\w]+$",
      "^notification.response$"
    ],
    "whitelistedSocketEvents": [
      "^barcode.start$",
      "^barcode.stop$",
      "^barcode.list$",
      "^config.translations.request$",
      "^config.languages.request$",
      "^config.features.request$",
      "^fbs.login$",
      "^fbs.patron$",
      "^fbs.checkout$",
      "^fbs.checkin$",
      "^fbs.renew$",
      "^fbs.renew.all$",
      "^notification.status$",
      "^notification.reservations$"
    ]
  },
  "translation": {
    "destination": "/files/translations.json"
  }
}
