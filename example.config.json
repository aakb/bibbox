{
  "port": 3010,
  "allowed": [ "127.0.0.1", "::ffff:192.168.50.1" ],
  "logs": {
    "info": "logs/info.log",
    "error": "logs/error.log",
    "debug": "logs/debug.log"
  },
  "proxy": {
    "busEvents": [
      "barcode.data",
      "barcode.err",
      "barcode.list.res",
      "config.translations",
      "frontend.reload"
    ],
    "proxyEvents": [
      "barcode.start",
      "barcode.stop",
      "barcode.list",
      "config.translations.request"
    ]
  },
  "translation": {
    "destination": "/files/translations.json"
  }
}
