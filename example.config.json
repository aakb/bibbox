{
  "port": 3010,
  "allowed": [ "127.0.0.1", "::ffff:192.168.50.1" ],
  "logs": {
    "info": "logs/info.log",
    "error": "logs/error.log",
    "debug": "logs/debug.log"
  },
  "proxy": {
    "whitelistedBusEvents": [
      "^barcode.data$",
      "^config.translations$",
      "^frontend.[\\w]+$",
      "^fbs.login.[\\w]+-[\\w]+$",
      "^fbs.patron.[\\w]+-[\\w]+$",
      "^fbs.checkout.[\\w]+-[\\w]+$",
      "^fbs.checkin.[\\w]+-[\\w]+$",
      "^fbs.renew.[\\w]+-[\\w]+$",
      "^fbs.renew.all.[\\w]+-[\\w]+$"
    ],
    "whitelistedSocketEvents": [
      "^barcode.start$",
      "^barcode.stop$",
      "^barcode.list$",
      "^config.translations.request$",
      "^fbs.login$",
      "^fbs.patron$",
      "^fbs.checkout$",
      "^fbs.checkin$",
      "^fbs.renew$",
      "^fbs.renew.all$"
    ]
  },
  "translation": {
    "destination": "/files/translations.json"
  }
}
