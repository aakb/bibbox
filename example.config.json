{
  "port": 3010,
  "logs": {
    "info": "logs/info.log",
    "error": "logs/error.log",
    "debug": "logs/debug.log"
  },
  "proxy": {
    "busEvents": [
      "barcode.data",
      "barcode.err",
      "barcode.list.res",
      "config.translations.res"
    ],
    "proxyEvents": [
      "barcode.start",
      "barcode.stop",
      "barcode.list",
      "config.translations"
    ]
  }
}
